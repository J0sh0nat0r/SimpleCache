{
  "name": "j0sh0nat0r/simple-cache",
  "description": "A simple yet modular PHP caching library",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "cache",
    "redis",
    "APC",
    "memcache",
    "file cache",
    "sqlite cache"
  ],
  "authors": [
    {
      "name": "J0sh0nat0r",
      "email": "josh@joshp.xyz"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.4.0"
  },
  "autoload": {
    "psr-4": {
      "J0sh0nat0r\\SimpleCache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "J0sh0nat0r\\SimpleCache\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
